{
    "name": "atoum/bdd-extension",
    "type": "library",
    "description": "atoum Spec BDD extension",
    "keywords": ["TDD", "atoum", "test", "unit testing", "spec", "bdd", "atoum-extension"],
    "homepage": "http://www.atoum.org",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "jubianchi",
            "email": "contact@jubianchi.fr"
        }
    ],
    "require": {
        "php": "^5.4.0 || ^7.0.0 || ^8.0.0"
    },
    "require-dev": {
        "atoum/atoum": "^2.9 || ^3.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "atoum\\atoum\\bdd\\": "classes"
        },
        "files": [
            "autoloader.php",
            "configuration.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "atoum\\atoum\\bdd\\specs\\": "specs"
        }
    }
}
